import java.util.Scanner;

class main{
    public static void main(String[] args){
        Scanner data = new Scanner(System.in);
        
        int num;
        System.out.println("Enter your number:");
        num = data.nextInt();
        
        while(num > 0){
            if (num % 2 == 0){
                System.out.println(num +  " even");
        
        }
             else {
                System.out.println(num +  " odd");
        }
        num--;
        }
    
        
        
        
       
    }
}
